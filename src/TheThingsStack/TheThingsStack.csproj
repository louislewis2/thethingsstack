<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>The Things Stack Integration Library</Description>
    <VersionPrefix>1.0.0-alpha-1</VersionPrefix>
    <Authors>Louis Lewis</Authors>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <AssemblyName>TheThingsStack</AssemblyName>
    <PackageId>TheThingsStack</PackageId>
    <PackageTags>TheThingsStack;TheThingsStack Integration</PackageTags>
    <PackageLicenseUrl>https://github.com/louislewis2/thethingsstack/blob/master/License.md</PackageLicenseUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/louislewis2/thethingsstack</RepositoryUrl>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    <PackageReleaseNotes>Preview 1</PackageReleaseNotes>
    <PackageProjectUrl>https://github.com/louislewis2/thethingsstack</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

</Project>
